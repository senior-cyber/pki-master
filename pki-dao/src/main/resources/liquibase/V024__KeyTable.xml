<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">
    <changeSet author="Senior Cyber" id="">

        <createTable tableName="tbl_key">
            <column autoIncrement="true" name="key_id" type="BIGINT">
                <constraints nullable="false" primaryKey="true"/>
            </column>

            <column name="client_id" type="VARCHAR(200)">
                <constraints unique="true" nullable="false"/>
            </column>
            <column name="client_secret" type="TEXT">
                <constraints nullable="false"/>
            </column>
            <column name="kek" type="TEXT">
                <constraints nullable="false"/>
            </column>
            <column name="user_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>

        </createTable>

        <createIndex tableName="tbl_key" indexName="">
            <column name="user_id"/>
        </createIndex>

    </changeSet>
</databaseChangeLog>